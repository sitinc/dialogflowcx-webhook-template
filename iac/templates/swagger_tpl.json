{
    "swagger": "2.0",
    "info": {
      "version": "0.0.1",
      "title": "${swagger_title}",
      "description": "${swagger_desc}",
      "termsOfService": "/static/tos.txt",
      "contact": {
        "name": "${contact_name}",
        "url": "${contact_url}",
        "email": "${contact_email}"
      },
      "license": "${app_license}"
    },
    "securityDefinitions": {
      "username": {
          "type": "apiKey",
          "in": "header",
          "name": "${hdr_auth_username}",
          "description": "The customAPI username authentication header.  Auto-generated by Terraform - DO NOT EDIT."
      },
      "apiKey": {
          "type": "apiKey",
          "in": "header",
          "name": "${hdr_auth_password}",
          "description": "The custom API password authentication header.  Auto-generated by Terraform - DO NOT EDIT."
      }
  },
    "servers": [
        {
          "url": "http://localhost:8080/",
          "description": "Development"
        },
        {
          "url": "https://${url_service_uat}/",
          "description": "User Acceptance Testing (UAT) environment.  Auto-generated by Terraform - DO NOT EDIT."
        },
        {
          "url": "https://${url_service_main}/",
          "description": "Production environment.  Auto-generated by Terraform - DO NOT EDIT."
        }
    ],
    "tags": [
      {
        "name": "Tests",
        "description": "Endpoints for testing."
      }
    ],
    "paths": {
        "/": {
          "post": {
            "security": [
              {"username": []},
              {"apiKey": []}
            ],
            "tags": ["Tests"],
            "summary": "Test the service.",
            "description": "Test the service.",
            "operationId": "testTheService",
            "parameters": [
                {
                  "in": "query",
                  "name": "testText",
                  "schema": { "type": "string" },
                  "default": "Hello",
                  "required": true,
                  "description": "The test text."
                }
            ],
            "consumes": ["application/json"],
            "produces": ["application/json"],
            "responses": {
              "200": {
                "description": "OK",
                "schema": {
                  "$ref": "#/definitions/RetVal"
                }
              }
            }
          }
        }
      },
      "definitions": {
        "RetVal": {
          "required": ["name", "_id", "companies"],
          "properties": {
            "transId": {"type": "string", "description": "The transaction ID."},
            "retval": {"type": "integer", "description": "The return code value. ZERO is for successful responses, NEGATIVE numbers are for system errors, and POSITIVE numbers are for user errors."},
            "retmsg": {"type": "string", "description": "The return message."}
          }
        }
      },
    "schemes": ["https", "http"],
    "consumes": ["application/json"],
    "produces": ["application/json"],
    "components": {
        "parameters": {
            "encodedBody": {
                "name": "encodedBody",
                "in": "body",
                "description": "The encoded body.",
