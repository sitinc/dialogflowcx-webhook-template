{
  "name": "@sitinc/dialogflowcx-webhook-template",
  "version": "0.0.2",
  "description": "Typescript/ExpressJS/Docker base template project for Dialogflow CX webhook API gateways.",
  "main": "dist/server.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/server.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/server.js\"",
    "coverage": "nyc npm run test",
    "fix": "eslint --fix --ext src/*.js --ext test/*.js",
    "lint": "eslint --ext src/*.js --ext test/*.js",
    "test": "mocha test/*.js --check-leaks"
  },
  "keywords": [],
  "author": "Justin Randall",
  "license": "MIT",
  "dependencies": {
    "@sitinc/dialogflowcx-tagexpress": "^0.0.1",
    "body-parser": "^1.20.2",
    "connect-timeout": "^1.9.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "nocache": "^4.0.0",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@types/connect-timeout": "^0.0.39",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/swagger-ui-express": "^4.1.6",
    "concurrently": "^8.2.2",
    "eslint": "^8.54.0",
    "eslint-config-google": "^0.14.0",
    "expect.js": "^0.3.1",
    "mocha": "^10.2.0",
    "mocha-param": "^2.0.1",
    "nodemon": "^3.0.1",
    "nyc": "^15.1.0",
    "typescript": "^5.3.2"
  }
}
